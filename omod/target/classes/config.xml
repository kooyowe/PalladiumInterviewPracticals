<?xml version="1.0" encoding="UTF-8"?>

<module configVersion="1.2">

	<!-- Module Properties -->
	<id>kenyaemrCharts</id>
	<name>KenyaEMR Adhoc Charts Module</name>
	<version>1.0-SNAPSHOT</version>
	<package>org.openmrs.module.kenyaemrCharts</package>
	<author>kenyaEMR Developers</author>
	<description>
		dhoc charts
	</description>
	<updateURL>https://modules.openmrs.org/modules/download/kenyaemrCharts/update.rdf</updateURL>
	<require_version>1.9.9</require_version>
	<!-- / Module Properties -->

	
	<!-- Module Activator -->
	<activator>org.openmrs.module.kenyaemrCharts.KenyaEMRAdhocChartsActivator</activator>

	<require_modules>
		<require_module version="15.1.4">org.openmrs.module.kenyaemr</require_module>
	</require_modules>
	
	<!-- Adds link to admin page -->
	<extension>
		<point>org.openmrs.admin.list</point>
		<class>org.openmrs.module.kenyaemrCharts.extension.html.AdminList</class>
	</extension>
	
	<!-- Maps hibernate file's, if present -->
	<mappingFiles>
				
	</mappingFiles>

	<!-- Internationalization -->
	<!-- All message codes should start with kenyaemrCharts. -->
	<messages>
		<lang>en</lang>
		<file>messages.properties</file>
	</messages>
	<messages>
		<lang>fr</lang>
		<file>messages_fr.properties</file>
	</messages>
	<messages>
		<lang>es</lang>
		<file>messages_es.properties</file>
	</messages>
	<!-- /Internationalization -->

</module>

